*/faça um programa que leia 2 nota de um aluno e calcule a media, caso a media seja inferior a 5 o aluno esta reprovado, caso o contrario o aluno esta aprovavado/*



<!doctype html>
<html lang="pt-br">
<head>
	<meta charset="utf-8"/>
	<title>HTML - Estrutura básica</title>
</head>
<body>

</body
</html>
<script>
	var num1 = parseInt(prompt("Digite o 1º valor:"));
	var num2 = parseInt(prompt("Digite o 2° valor:"));
	var res = num1 - num2;
	
	if(res > 0) {
		alert("Numero " + res + " positivo");
	} else if(res < 0) {
		alert("Numero " + res + " negativo");
	} else {
		alert("Numero " + res + " é ZERO");
	}
	
	
</script>